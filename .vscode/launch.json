{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Jest Current File",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\node_modules\\jest\\bin\\jest",
      "args": [
        "${fileBasenameNoExtension}"
      ],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "name": "Run Current File",
      "request": "launch",
      "program": "${workspaceFolder}\\src\\template\\2-js-template.js"
    }
  ]
}